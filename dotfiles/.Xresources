URxvt.font:                       xft:Inconsolata\ Nerd\ Font\ Mono:style=Medium:size=13:antialias=true
URxvt.boldFont:                   xft:Inconsolata\ Nerd\ Font\ Mono:style=Medium:size=13:antialias=true
URxvt.letterSpace:                -0.5

URxvt*colorUL:                    yellow
URxvt*loginShell:                 true
URxvt*scrollTtyOutput:            false
URxvt*scrollWithBuffer:           true
URxvt*scrollTtyKeypress:          true
URxvt*skipScroll:                 false
URxvt.url-launcher:               google-chrome-stable
URxvt.matcher.button:             3
URxvt*depth:                      32
URxvt*background:                 [90]#2b303b
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                true
URxvt*foreground:                 #c0c5ce
URxvt*saveLines:                  65535
URxvt.keysym.C-x: perl:matcher:list
URxvt.searchable-scrollback: C-f

URxvt.perl-ext-common: default,one-based-matcher,selection-to-clipboard,resize-font

! Normal copy-paste keybindings without perls
URxvt.iso14755:                   false
URxvt.keysym.Shift-Control-V:     eval:paste_clipboard
URxvt.keysym.Shift-Control-C:     eval:selection_to_clipboard
!Xterm escape codes, word by word movement
URxvt.keysym.Control-Left:        \033[1;5D
URxvt.keysym.Shift-Control-Left:  \033[1;6D
URxvt.keysym.Control-Right:       \033[1;5C
URxvt.keysym.Shift-Control-Right: \033[1;6C
URxvt.keysym.Control-Up:          \033[1;5A
URxvt.keysym.Shift-Control-Up:    \033[1;6A
URxvt.keysym.Control-Down:        \033[1;5B
URxvt.keysym.Shift-Control-Down:  \033[1;6B

